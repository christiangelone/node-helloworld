{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [{
    "name": "node-helloworld",
    "image": "<AWS_ACCOUNT_ID>.dkr.ecr.eu-west-1.amazonaws.com/christiangelone/node-helloworld:<VERSION_TAG>",
    "essential": true,
    "memoryReservation": 1024,
    "portMappings": [{
      "hostPort": 3333,
      "containerPort": 3333
